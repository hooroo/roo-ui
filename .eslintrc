{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
  },
  "env": {
    "jest": true
  },
  "overrides": [
    {
      "files":[
        "**/*.test.js"
      ],
      "rules": {
        "jsx-a11y/label-has-for": 0
      }
    }
  ]
}
