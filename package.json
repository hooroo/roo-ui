{
  "name": "roo-ui",
  "description": "Qantas Hotels' library of accessible React components and assets",
  "repository": "git@github.com:hooroo/roo-ui.git",
  "author": "Hooroo <support@hooroo.com>",
  "license": "MIT",
  "main": "lib/",
  "types": "lib/index.d.ts",
  "module": "esm/",
  "files": [
    "!**/icons/**/",
    "lib/",
    "esm/",
    "assets/",
    "components/",
    "fonts/",
    "icons/",
    "logos/",
    "*.d.ts",
    "!*/icons/*/",
    "!*.test.*",
    "!*.story.*"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "build-icons": "scripts/build-icons",
    "build:entrypoints": "scripts/build-entrypoints",
    "build:cjs": "MODULES=commonjs babel src -d lib --copy-files",
    "build:esm": "MODULES=false babel src -d esm --copy-files",
    "build": "npm-run-all build-icons --parallel build:*",
    "pretest": "yarn build-icons",
    "release": "semantic-release",
    "storybook": "yarn build-icons && start-storybook",
    "build-storybook": "yarn build-icons && build-storybook",
    "generate-component": "hygen component new --path",
    "generate-entrypoint": "HYGEN_OVERWRITE=1 hygen entrypoint new --name"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "modulePathIgnorePatterns": [
      "lib/",
      "esm/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-options": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/react": "^16.9.2",
    "@types/styled-system": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.17",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-sonarjs": "^0.4.0",
    "husky": "^3.0.5",
    "hygen": "^4.0.9",
    "is-var-name": "^2.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-emotion": "^10.0.17",
    "lint-staged": "^9.2.5",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-modal": "^3.10.1",
    "react-onclickoutside": "^6.9.0",
    "react-popper": "^1.3.4",
    "react-powerplug": "^1.0.0",
    "react-text-mask": "^5.4.3",
    "semantic-release": "17.0.1",
    "storybook-readme": "^5.0.8",
    "styled-system": "^4.1.0",
    "svgi": "^1.1.0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^0.8.2",
    "date-fns": "^2.5.0",
    "dayzed": "^3.0.0",
    "emotion-theming": "^10.0.17",
    "lodash": "^4.17.15",
    "polished": "^3.6.5",
    "react-modal": "^3.10.1",
    "react-onclickoutside": "^6.9.0",
    "react-popper": "^1.3.4",
    "react-text-mask": "^5.4.3"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.0",
    "@emotion/styled": "^10.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-system": "^4.0.0"
  }
}
